{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Install auth deps",
			"type": "shell",
			"command": "npm",
			"args": [
				"install"
			],
			"isBackground": false,
			"problemMatcher": [
				"$eslint-stylish"
			],
			"group": "build"
		},
		{
			"label": "Prisma generate",
			"type": "shell",
			"command": "npx",
			"args": [
				"prisma",
				"generate"
			],
			"isBackground": false,
			"problemMatcher": [
				"$eslint-stylish"
			],
			"group": "build"
		},
		{
			"label": "Prisma migrate dev",
			"type": "shell",
			"command": "npx",
			"args": [
				"prisma",
				"migrate",
				"dev",
				"--name",
				"auth-init"
			],
			"isBackground": false,
			"problemMatcher": [
				"$eslint-stylish"
			],
			"group": "build"
		},
		{
			"label": "Prisma db push",
			"type": "shell",
			"command": "npx",
			"args": [
				"prisma",
				"db",
				"push"
			],
			"isBackground": false,
			"problemMatcher": [
				"$eslint-stylish"
			],
			"group": "build"
		}
	]
}